generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id          String   @id @default(nanoid())
    email       String?
    thai_id     String   @unique
    phone       String
    firstname   String
    lastname    String
    gender      String
    birthday    DateTime
    address     String
    subdistrist String
    district    String
    province    String
    postcode    String
}
